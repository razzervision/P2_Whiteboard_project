<body>
<div>
  <button type="button" id="buttonPDF" onclick="convertHTMLtoPDF()">Click to download PDF</button>
</div>

<div id="webPage">

  <div class="toolbar saveToPDF" id="toolbar">

    <nav>
      <menu id="toolbarMenu">
        <div class = "toolbarElement" id = "codeEditorElement" data-value="codeProgram" draggable="true">Code Editor</div>
        <div class = "toolbarElement" id = "quizElement" data-value="quizProgram" draggable="true">Quiz</div>
        <div class = "toolbarElement" id = "calculatorElement" data-value="calcProgram" draggable="true">Calc</div>
        <div class="toolbarDivForCheckbox" id="socketDiv"><input class="oolbarCheckboxInput" type="checkbox" id="connectSocket"> <label class="toolbarLabelForCheckbox" for="connectSocket">connect to group</label><t/div>
      </menu>
    </nav>
  </div>

  <div id="primaryDropzone">
    <!-- <div id="drawArea">
      
    </div> -->
  </div>



<div id="quizProgram" class="quiz_container saveToPDF">

  <button class="quiz_container_button" id="quiz_home_screen">HOME</button>
  <button id = "quiz_min_button" class="minimizeButton" data-value="quizProgram">--</button>
  
  <div class="quiz_index" id="quiz_index">
    <button id="createQuizIndex">Create Quiz</button>
    <br>
    <button id="joinQuizIndex">Join Quiz</button>
    <br>
    <button id="searchQuizIndex">Search Existing Quiz</button>
  </div>

    <div id="createQuizDiv" style="display: none;">
      <label for="create_quiz_text">Quiz name</label>
      <input type="text" id="create_quiz_text" placeholder="e.g Mergesort">
    <button id="create_quiz_button">Create quiz</button>
    </div>

    <div id="joinQuizDiv" style="display: none;">
      <label for="join_quiz_text">Join Quiz</label>
      <input type="text" id="join_quiz_text" placeholder="e.g 111q2">
      <label for="quizUsername">Join Quiz</label>
      <input type="text" id="quizUsername" placeholder="John Johnsen">
      <button id="join_quiz_button">Join quiz</button>
    </div>
    
    <!-- Removed because it should not be possible for everyone to see everyones score -->
    <!-- <div id="joinQuizSession">
      <label for="join_quiz_session">Insert session</label>
      <input type="text" id="join_quiz_session" placeholder="e.g 111q2">
      <button id="join_quiz_session_button">Join session results</button>
    </div> -->
    <div id="searchQuizDiv" style="display: none;">
      <label for="search_quiz_text">Search quizzes</label> 
      <input type="text" id="search_quiz_text" placeholder="e.g Mergesort">
  
      <table id="search_quiz_table">
        <tr>
          <th>Quiz name</th>
          <th>Start Quiz</th>
        </tr>
      </table>
    </div>

  <div class="creat_quiz_div" id="creat_quiz_div" style="display: none;">
    <label for="quiz_name">Quiz Name:</label>
    <a id="quiz_name"></a><br><br>
    <button id="append_new_question">New Question =></button><br>
    <button id="upload_quiz_button">Upload Quiz</button><br>
  </div>
  <div id="quiz_output" style="display: none;">

  </div>
  <div id="quiz_output_results">

  </div>
 

</div>

  <div id="paintProgram" class="paint saveToPDF">
  <div id = "canvasPlace">  
    <canvas id = "canvas0"></canvas>
  </div>
 
    <div id = "nav" class="nav">
            <div class ="options" id ="optionsForPaint">
                <input type="file" id="uploadInput" accept="image/*">
                <input type="number" id="Imgheight">
                <input type="number" id="Imgwithd">
                <button id ="addCanvas" type="button">Add Canvas</button>
                <button id="picture location">Click to chose picture location</button>
                <button id = "clearCanvas">Clear canvas</button>
                <input oninput="drawColor = this.value" id = "colorpick" type = "color" class="colorpick">
                <input oninput="drawWithd = this.value" type="range" min="1" max="100" class="pen">
                <button id ="undoB" type ="button">Undo</button>
                <button id ="changeCanvas" type="button" class = "canvasButton">C0</button>
            </div>
    </div>
  </div>


  <!-- JONS code editor -->
  <div id="codeProgram" class="code_editor saveToPDF">
    <div id="codeProgramBar">
      <button class="minimizeButton" data-value="codeProgram">--</button>
    </div>
  <textarea id="code"></textarea>
  <select id="language">
  </select>
  <select id="theme">
      <option value="darcula">Darcula</option>
      <option value="solarized">Solarized</option>
      <option value="oceanic-next">Oceanic Next</option>
      <option value="shadowfox">Shadowfox</option>
      <option value="monokai">Monokai</option>
  </select>
</div>


<!--- calDIV -->
<div id="calcProgram" class="calculatorMain saveToPDF"> 
  <div class="dropdownCalc">
    <select name="mathType" id="mathType">
    <option value="calculator">Calc</option>
    <option value="matricesMath">Matrices</option>
    <!-- tilføj flere her-->
    </select>
    <div class="minimizeCalc">
      <button class="minimizeCalcButton">-</button>
    </div>
  </div>

<!--- CALC -->
  <div class="calculator">
    <div class="expressions">
      <input type="text" id="display" class="display"> <br>
      <button class="calcbuttonDelete" id="C" onclick="clearDisplay()">C</button> 
      <button class="calcbuttonCalc" id="+/-" onclick="negative('-')">+/-</button>
      <button class="calcbuttonCalc" id="*" onclick="addToResult('*')">*</button> 
      <button class="calcbuttonCalc" id="+" onclick="addToResult('+')">+</button> <br>
      <button class="calcbuttonKey" id="1" onclick="addToResult('1')">1</button>
      <button class="calcbuttonKey" id="2" onclick="addToResult('2')">2</button>
      <button class="calcbuttonKey" id="3" onclick="addToResult('3')">3</button>
      <button class="calcbuttonCalc" id="^" onclick="addToResult('^')">^</button>
      <button class="calcbuttonCalc" id="log" onclick="addToResult('log()')">log2</button> <br>
      <button class="calcbuttonKey" id="4" onclick="addToResult('4')">4</button>
      <button class="calcbuttonKey" id="5" onclick="addToResult('5')">5</button>
      <button class="calcbuttonKey" id="6" onclick="addToResult('6')">6</button> 
      <button class="calcbuttonCalc" id="-" onclick="addToResult('-')">-</button> <br>
      <button class="calcbuttonKey" id="7" onclick="addToResult('7')">7</button>
      <button class="calcbuttonKey" id="8" onclick="addToResult('8')">8</button>
      <button class="calcbuttonKey" id="9" onclick="addToResult('9')">9</button>
      <button class="calcbuttonCalc" id="/" onclick="addToResult('/')">/</button> <br>
      <button class="calcbuttonKey" id="0" onclick="addToResult('0')">0</button>
      <button class="calcbuttonCalc" id="%" onclick="addToResult('%')">%</button>
      <button class="calcbuttonCalc" id="*" onclick="addToResult('*')">*</button> 
      <button class="calcbuttonCalc" id="!" onclick="addToResult('!')">!</button> <br>
      <button class="calcbuttonSubmit" id="submit" onclick="calculate()">Submit</button> 
    </div>

    <div class="calcHistory">
      <textarea id="classHistory" class="classHistory" cols="40" rows="10"></textarea> <br>
      <button id="clearHistoryButton" class="clearHistoryButton" onclick="clearHistory()">CLEAR ALL HISTORY</button> 
    </div>

    
  </div>
  <!--- Matrice calculation--->
  <div class="matrices">
      <p class="matriceTxtStyle">Please enter the dimensions of the matrices: Rows: </p>
      <input type="text" id="xMatrice" class="xMatrice">
      <p class="matriceTxtStyle">Columns:</p> 
      <input type="text" id="yMatrice" class="yMatrice">
      <button class="submitDimensions" onclick="createDimension()">add matrice</button>
      <button class="RestartDimensions" onclick="restartDimension()">Restart Dimensions</button> <br>
        <div class="matriceCalculation0">

        </div>

        <div class="matriceOperator">
          <select name="matriceOp" id="matriceOp" class="matriceOp">
            <option value="plus">+</option>
            <option value="minus">-</option>
            <option value="times">×</option>
            <option value="divide">/</option>
          </select>
        </div>
        <div class="matriceCalculation1">

          
        </div>
  </div>
</div>





<!--- TIMER -->
<div class="saveToPDF" id="timerProgram">
  <!--This is the trigger for the modal-->       
  <div class="stickymodal"> 
    <button id="pauses"> Click here for pause options </button>
  </div>

  <!--this is the modal-->        
  <div id="myModalforPauses" class="modal"> 

  <!--this is the modal content-->

     <div class="modal-content"> 
          <span class="close">&times;</span>
             <p><ul>
             <li><button id="longwork" class="timer" onclick="timer(1,30,0)" >Long work, long break </button></li> 
             <li><button id="shortwork" class="timer" onclick="timer(0,45,0)" >Short work, short break </button></li>
             <li><button id="chore" class="timer" onclick="timer(0,0,5);PauseTimerArgument(0,0,9)">Finish a chore! </button></li>
             <li><button id="relax" class="timer" onclick="timer(0, 0, 5)" onclick="ClaireDeLune(0,0,5)" onclick>Relax break</button></li>
             <li><button id="other" class="timer" onclick="timer(5)" >other</button></li>
             </ul></p>
      </div>
   </div>


    <!--Moved a </div> from here to under the calc, since it does not pertain to this code-->
     
    <!--This is the trigger for the modal-->
    <div id="sc" class="stickyclock"> 
      <div class="clock"> 
        <button id="clockbutton"> 
        <span id="hrs">00</span>
        <span>:</span>
        <span id="min">00</span>
        <span>:</span>
        <span id="sec">00</span>
        </button>
      </div> 
     </div> 

      <!--This is the second modal-->
    <div id="Modalforclockbutton" class="modal">

      <!--This is the modal content-->
      <div class="modal-content"> 
        <span class="close">&times;</span>
           <p><ul>
           <li><button id="longwork" onclick="timer()" >Continue</button></li> 
           <li><button id="shortwork" onclick="timer()" >Reset</button></li>
           </ul></p>
    </div>
     </div>

    </div>


<script src="/javascripts/timer.js"></script>
<script src="https://cdn.socket.io/4.7.5/socket.io.min.js"></script>
<script src="/javascripts/socket.js"></script>
